{
    "manifest_version" : 2,
    "name" : "TimeZone / App Finder",
    "version" : "1.0.0",
    "description" : "Extension to ",
    "icons" : {
        "128" : "clock.png"
    },
    "browser_action" : {
        "default_title" : "TimeZone / App Finder",
        "default_popup" : "index.html",
        "default_icon" : "clock.png"
    },
    "permissions" : ["activeTab"],
    "background" : {
        "scripts" : ["app.js"]
    }
}